{
  "name": "data-vis",
  "version": "0.0.1",
  "description": "data-vis",
  "main": "src/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BertrandFritsch/data-vis.git"
  },
  "private": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "vars": "env | grep npm_package_",
    "build": "NODE_ENV=production PUBLIC_PATH=/coursera-data-vis1 webpack -p",
    "build:watch": "NODE_ENV=development webpack-dev-server --progress",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "test:unit:watch": "NODE_ENV=test jest --watch",
    "test:unit": "NODE_ENV=test jest --coverage --coverageReporters lcov",
    "tsc": "tsc -p ./tsconfig.all.json --noEmit --pretty"
  },
  "dependencies": {
    "carbon-components": "^8.10.0",
    "carbon-icons": "^6.2.0",
    "d3": "^4.13.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@types/d3": "^4.12.0",
    "@types/enzyme": "^3.1.8",
    "@types/html-webpack-plugin": "^2.30.1",
    "@types/jest": "^22.1.2",
    "@types/prop-types": "^15.5.2",
    "@types/react-dom": "^16.0.3",
    "@types/react-hot-loader": "^3.0.5",
    "@types/react-redux": "^5.0.14",
    "@types/webpack": "^3.8.4",
    "@types/webpack-env": "^1.13.5",
    "@types/webpack-notifier": "^1.5.1",
    "autoprefixer": "^7.2.5",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "file-loader": "^1.1.6",
    "fork-ts-checker-webpack-plugin": "^0.3.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.2.2",
    "json-loader": "^0.5.7",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.0",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.1",
    "ts-jest": "^22.0.3",
    "ts-loader": "^3.4.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-notifier": "^1.5.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "**/*.spec.(ts|tsx)"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|pdf|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      ".*\\.(s?css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "./jest/requestAnimationFrame.ts",
      "./jest/jsdomSetup.ts",
      "./jest/jestSetup.ts"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "roots": [
      "<rootDir>"
    ],
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    },
    "mapCoverage": true,
    "collectCoverageFrom": [
      "src/{contexts}/**/*.{ts,tsx}",
      "!src/{contexts}/**/*.spec.{ts,tsx}"
    ]
  }
}
