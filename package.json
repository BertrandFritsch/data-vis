{
  "name": "data-vis",
  "version": "0.0.1",
  "description": "data-vis",
  "main": "src/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BertrandFritsch/data-vis.git"
  },
  "private": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "vars": "env | grep npm_package_",
    "build": "webpack --env.PUBLIC_PATH=/focusable-timeline --env.NODE_ENV=production -p",
    "build:watch": "webpack-dev-server --env.NODE_ENV=development --progress -d",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "test:unit:watch": "NODE_ENV=test jest --watch",
    "test:unit": "NODE_ENV=test jest --coverage --coverageReporters lcov",
    "tsc": "tsc -p ./tsconfig.all.json --noEmit --pretty"
  },
  "dependencies": {
    "@ibm/plex": "^1.0.1",
    "carbon-components": "^8.18.0",
    "carbon-icons": "^6.3.2",
    "d3": "^5.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@types/d3": "^5.0.0",
    "@types/enzyme": "^3.1.9",
    "@types/html-webpack-plugin": "^2.30.3",
    "@types/jest": "^22.2.2",
    "@types/prop-types": "^15.5.2",
    "@types/react-dom": "^16.0.4",
    "@types/react-hot-loader": "^3.0.6",
    "@types/react-redux": "^5.0.15",
    "@types/webpack": "^4.1.2",
    "@types/webpack-env": "^1.13.5",
    "@types/webpack-notifier": "^1.5.3",
    "autoprefixer": "^8.2.0",
    "babel-loader": "^7.1.4",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "html-webpack-plugin": "^3.1.0",
    "jest": "^22.4.3",
    "node-sass": "^4.8.3",
    "postcss-loader": "^2.1.3",
    "react-hot-loader": "^4.0.0",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.7",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.3",
    "ts-jest": "^22.4.2",
    "ts-loader": "^4.1.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1",
    "webpack-notifier": "^1.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "**/*.spec.(ts|tsx)"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|pdf|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      ".*\\.(s?css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "./jest/requestAnimationFrame.ts",
      "./jest/jsdomSetup.ts",
      "./jest/jestSetup.ts"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "roots": [
      "<rootDir>"
    ],
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    },
    "collectCoverageFrom": [
      "src/{contexts}/**/*.{ts,tsx}",
      "!src/{contexts}/**/*.spec.{ts,tsx}"
    ]
  }
}
